generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Auth {  
  user_id          String   @id @default(cuid())
  token            String   @unique
  expiryDate       DateTime
}